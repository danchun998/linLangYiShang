page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

.custom-van-nav-bar{
  padding: 0 !important;
}

.list-container{
  display: flex;
  flex-wrap: wrap;
  /* justify-content: space-around; */
}

.list-img{
  width: 200rpx;
  height: 200rpx;
  background-color: #eeeeee;
  margin-bottom: 20rpx;
  margin-left: 37.5rpx;
  border: 1rpx #cccccc solid;
  position: relative;
}

.list-img image{
  width: 100%;
  height: 100%;
}

.list-img .remove-img{
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background: rgba(224,224,224,0.7);
  position: absolute;
  z-index: 2;
  top: -15rpx;
  right: -15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: rgba(88,88,88,0.8);
}

.chooseImgBtn{
  display: flex;
  justify-content: center;
  align-items: center;
}